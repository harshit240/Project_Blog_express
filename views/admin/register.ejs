<%- include('../front/header.ejs') %>


<section class="login-wrap mtb-40">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-md-6">
                <div class="login-box">
                    <h1>Administrator Login</h1>
                    <form action="">
                        <div class="form-group">
                            <input type="text" placeholder="Name" name ="name" class="form-control mt-3">
                        </div>
                        <div class="form-group">
                            <input type="text" placeholder="Email" name="email" class="form-control mt-3">
                        </div>
                        <div class="form-group">
                            <input type="text" placeholder="Password" name="password" class="form-control mt-3">
                        </div>
                        <div class="form-group">
                            <input type="text" placeholder="Confirm Password" name="confirm_password" class="form-control mt-3">
                        </div>
                        <div class="form-group">
                            <button class="btn btn-info mt-3 form-control mb-3 ">Register</button> 
                        </div>
                    </form>
                </div>         
            </div>
        </div>
    </div>
</section>
